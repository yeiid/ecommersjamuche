{
  "name": "ecommersjamuche",
  "type": "module",
  "version": "0.1.0",
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build --verbose && node scripts/fix-vercel-runtime.js",
    "postbuild": "echo '{ \"runtime\": \"nodejs20.x\" }' > .vercel/output/functions/_render.func/.vc-config.json",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=junit --outputFile=./test-results.xml",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "analyze": "astro build --analyzer",
    "check": "astro check"
  },
  "dependencies": {
    "@astrojs/svelte": "^7.0.7",
    "@astrojs/tailwind": "^6.0.1",
    "@astrojs/vercel": "3.8.1",
    "astro": "^5.5.4",
    "node-fetch": "2",
    "svelte": "^5.23.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.49.3",
    "@unpic/astro": "^1.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-svelte": "^2.35.0",
    "happy-dom": "^13.5.0",
    "jsdom": "^26.0.0",
    "lucide-svelte": "^0.483.0",
    "motion": "^12.5.0",
    "nanostores": "^0.11.4",
    "postcss": "^8.4.32",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "vitest": "^1.2.0"
  }
}
